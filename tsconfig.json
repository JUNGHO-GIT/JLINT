{
  "$schema": "https://www.schemastore.org/tsconfig",
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
		"target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
    ],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"verbatimModuleSyntax": true,
		"incremental": true,
		"tsBuildInfoFile": "./out/.tsbuildinfo"
	}
}