{
  "name": "jlint",
  "displayName": "Jlint",
  "description": "Universal Language formatter",
  "publisher": "JUNGHO",
  "icon": "logo.webp",
  "license": "Apache-2.0",
  "main": "./out/extension.js",
  "version": "2.7.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/JUNGHO-GIT/JLINT.git"
  },
  "categories": [
    "Formatters",
    "Linters",
    "Programming Languages",
    "Snippets",
    "Language Packs",
    "Extension Packs",
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "lint",
    "formatter",
    "jlint",
    "code",
    "format",
    "prettier",
    "java",
    "xml",
    "html",
    "css",
    "javascript",
    "typescript",
    "json",
    "yaml",
    "sql",
    "jsp",
    "universal"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "outputChannels": [
      "Jlint"
    ],
    "commands": [
      {
        "command": "extension.Jlint",
        "title": "Jlint"
      }
    ],
    "keybindings": [
      {
        "command": "extension.Jlint",
        "key": "alt+shift+f",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "extension.Jlint",
          "when": "editorTextFocus"
        }
      ],
      "editor/context": [
        {
          "command": "extension.Jlint",
          "when": "editorTextFocus"
        }
      ]
    },
    "configuration": {
      "title": "Jlint",
      "properties": {
        "Jlint.logLevel": {
          "type": "string",
          "scope": "window",
          "enum": [
            "off",
            "debug",
            "info",
            "warn",
            "error"
          ],
          "default": "info"
        },
        "Jlint.activateLint": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable the lint",
          "scope": "resource"
        },
        "Jlint.removeComments": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable the remove comments",
          "scope": "resource"
        },
        "Jlint.insertLine": {
          "type": "boolean",
          "default": false,
          "description": "Enable or disable the insert 'vertical line'",
          "scope": "resource"
        },
        "Jlint.tabSize": {
          "type": "number",
          "enum": [
            2,
            4
          ],
          "default": 2,
          "description": "Set the tab size for formatting (2 or 4 spaces)",
          "scope": "resource"
        },
        "Jlint.quoteType": {
          "type": "string",
          "enum": [
            "single",
            "double"
          ],
          "default": "double",
          "description": "Set the quote type for strings (single: ', double: \")",
          "scope": "resource"
        }
      }
    }
  },
  "engines": {
    "vscode": ">=1.106.1",
    "node": ">=21.0.0",
    "npm": ">=10.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "start-client": "bun .node/swc.cjs --bun --start --client",
    "start-server": "bun .node/swc.cjs --bun --start --server",
    "build-client": "bun .node/swc.cjs --bun --build --client",
    "build-server": "bun .node/swc.cjs --bun --build --server",
    "git-fetch": "bun .node/git.cjs --bun --fetch",
    "git-push": "bun .node/git.cjs --bun --push",
    "fix": "bun .node/fix.cjs --bun --fix",
    "reset": "bun .node/reset.cjs --bun --reset",
    "vsce": "bun .node/vsce.cjs --bun --package",
    "gcloud": "bun .node/gcloud.cjs --bun --deploy"
  },
  "dependencies": {
    "@prettier/plugin-xml": "3.4.2",
    "astring": "1.9.0",
    "cheerio": "1.1.2",
    "chevrotain": "11.0.3",
    "clean-css": "5.3.3",
    "fast-xml-parser": "5.3.2",
    "html-minifier-terser": "7.2.0",
    "jsdom": "27.2.0",
    "lodash": "4.17.21",
    "minimatch": "10.1.1",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-java": "2.7.7",
    "prettier-plugin-jsp": "1.0.13",
    "sql-formatter": "15.6.10",
    "strip-comments": "2.0.1",
    "strip-json-comments": "5.0.3",
    "terser": "5.44.1",
    "yaml": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.3",
    "@swc/helpers": "0.5.17",
    "@types/clean-css": "4.2.11",
    "@types/html-minifier-terser": "7.0.2",
    "@types/jsdom": "27.0.0",
    "@types/lodash": "4.17.21",
    "@types/mocha": "10.0.10",
    "@types/node": "24.10.1",
    "@types/strip-comments": "2.0.4",
    "@types/vscode": "1.106.1",
    "@vscode/test-electron": "2.5.2",
    "@vscode/vsce": "3.7.1",
    "eslint": "9.39.1",
    "mocha": "11.7.5",
    "ts-morph": "27.0.2",
    "esbuild": "0.27.0",
    "ts-prune": "0.10.3",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3"
  }
}
