{
  "name": "JLINT",
  "displayName": "JLINT",
  "description": "Custom Linting Program for me",
  "version": "1.0.0",
  "publisher": "JUNGHO",
  "icon": "JLINT.png",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.JLINT"
  ],
  "categories": [
    "Formatters",
    "Linters"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.JLINT",
        "title": "JLINT"
      }
    ],
    "keybindings": [
      {
        "command": "extension.JLINT",
        "key": "alt+shift+f",
        "when": "editorTextFocus && !editorReadonly && !suggestWidgetVisible"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "vsce": "vsce package"
  },
  "dependencies": {
    "fast-xml-parser": "^4.2.2",
    "glob": "^10.2.3",
    "lodash": "^4.17.21",
    "minimatch": "^3.0.4",
    "prettier": "^2.8.8",
    "prettier-plugin-java": "^2.1.0",
    "prettier-plugin-xml": "^0.1.0",
    "strip-comments": "^2.0.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.11.12",
    "@types/prettier": "^2.7.2",
    "@types/strip-comments": "^2.0.1",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "htmlparser2": "^9.0.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  }
}
